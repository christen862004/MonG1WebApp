<!--List<Student>-->

<table class="table table-bordered table-hover">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Image</th>
    </tr>
    @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ImageURL</td>
            <td>
                <img src="/Images/@item.ImageURL" style="width:100px" />
            </td>
        </tr>
    }

</table>


